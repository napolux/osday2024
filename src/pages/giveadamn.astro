---
import SlideLayout from "../layouts/SlideLayout.astro";
import { KeyboardManager } from "../components/react/KeyboardManager";
import Hero from "../components/astro/Hero.astro";

const title = "I said to myself...";
---

<SlideLayout pageTitle={title}>
  <Hero transition:name="ageism">Can't do much about it 🤷‍♂️</Hero>
  <KeyboardManager prev="/ageism" next="/" client:only />
</SlideLayout>
