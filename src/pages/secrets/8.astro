      <Left proportions="w-full">
        <List>
          <li>lorem ipsum</li>
        </List>
      </Left>
    </Content>
  </SlideLeftRight>
  <KeyboardManager prev="/secrets/7" next="/secrets/9" client:only />
</SlideLayout>
